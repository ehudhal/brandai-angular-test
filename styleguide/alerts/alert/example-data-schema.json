{
  "alertType": {
    "label": "Alert level",
    "type": "select",
    "values": [
      {
        "label": "Success",
        "value": "success"
      },
      {
        "label": "Info",
        "value": "info"
      },
      {
        "label": "Warning",
        "value": "warning"
      },
      {
        "label": "Danger",
        "value": "danger"
      }
    ]
  },
  "dismissable": {
    "type": "attribute",
    "key": "close",
    "value": {
      "label": "Dismissable",
      "type": "switch",
      "on": "toggleAlert()"
    }
  }
}
