{
  "timepickerAttrs": {
    "type": "group",
    "values": [
      {
        "type": "attribute",
        "key": "hour-step",
        "value": {
          "label": "Hour step",
          "type": "number",
          "min": 1,
          "max": 3,
          "step": 1,
          "default": 1
        }
      },
      {
        "type": "attribute",
        "key": "minute-step",
        "value": {
          "label": "Minute step",
          "type": "select",
          "values": [
            "1",
            "5",
            "10",
            "15",
            "20",
            "25",
            "30"
          ]
        }
      },
      {
        "type": "attribute",
        "key": "show-meridian",
        "value": {
          "label": "Show meridian",
          "type": "switch",
          "off": "false",
          "default": "on"
        }
      },
      {
        "type": "attribute",
        "key": "readonly-input",
        "value": {
          "label": "Read only",
          "type": "switch",
          "on": "true"
        }
      },
      {
        "type": "attribute",
        "key": "mousewheel",
        "value": {
          "label": "Enable mouse wheel",
          "type": "switch",
          "off": "false",
          "default": "on"
        }
      },
      {
        "type": "attribute",
        "key": "arrowkeys",
        "value": {
          "label": "Enable arrow keys",
          "type": "switch",
          "off": "false",
          "default": "on"
        }
      }
    ]
  }
}
