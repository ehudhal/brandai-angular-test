{
  "hourStep": {
    "label": "Hour step",
    "type": "number",
    "min": 1,
    "max": 3,
    "step": 1
  },
  "minStep": {
    "label": "Minute step",
    "type": "enum",
    "options": [
      "1",
      "5",
      "10",
      "15",
      "20",
      "25",
      "30"
    ]
  },
  "showMeridian": {
    "label": "12 or 24 hour",
    "type": "enum",
    "options": [
      {
        "label": "12 hour",
        "value": "true"
      },
      {
        "label": "24 hour",
        "value": "false"
      }
    ]
  },
  "readonly": {
    "label": "Read only",
    "type": "optional-string",
    "value": " readonly-input=\"true\""
  },
  "mousewheel": {
    "label": "Enable arrow keys",
    "type": "optional-string",
    "value": " mousewheel=\"true\""
  },
  "arrowkeys": {
    "label": "Enable mousewheel",
    "type": "optional-string",
    "value": " arrowkeys=\"true\""
  }
