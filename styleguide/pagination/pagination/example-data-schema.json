{
  "paginationAttrs": {
    "type": "group",
    "values": [
      {
        "type": "attribute",
        "key": "class",
        "value": {
          "type": "group",
          "values": [
            {
              "type": "const",
              "value": "pagination"
            },
            {
              "label": "Size",
              "type": "select",
              "optional": true,
              "values": [
                {
                  "label": "Large",
                  "value": "pagination-lg"
                },
                {
                  "label": "Small",
                  "value": "pagination-sm"
                }
              ]
            }
          ]
        }
      },
      {
        "type": "attribute",
        "key": "boundary-links",
        "value": {
          "label": "Show first and last buttons",
          "type": "switch",
          "on": "true"
        }
      },
      {
        "type": "attribute",
        "key": "direction-links",
        "value": {
          "label": "Show previous and next buttons",
          "type": "switch",
          "off": "false",
          "default": "on"
        }
      },
      {
        "type": "attribute",
        "key": "rotate",
        "value": {
          "label": "Keep current page in the middle",
          "type": "switch",
          "off": "false",
          "default": "on"
        }
      },
      {
        "type": "attribute",
        "key": "total-items",
        "value": {
          "label": "Total items",
          "type": "number",
          "min": 10,
          "max": 100,
          "step": 10
        }
      },
      {
        "type": "attribute",
        "key": "items-per-page",
        "value": {
          "label": "Items per page",
          "type": "number",
          "min": 5,
          "max": 30,
          "step": 5
        }
      },
      {
        "type": "attribute",
        "key": "max-size",
        "value": {
          "label": "Maximum pages shown",
          "type": "number",
          "min": 5,
          "max": 30,
          "step": 5
        }
      }
    ]
  },
  "prevNextLabels": {
    "type": "linked-select",
    "values": [
      {
        "label": "Previous / Next",
        "values": {
          "previousText": "",
          "nextText": ""
        }
      },
      {
        "label": "‹ / ›",
        "values": {
          "previousText": "&lsaquo;",
          "nextText": "&rsaquo;"
        }
      },
      {
        "label": "← / →",
        "values": {
          "previousText": "&larr;",
          "nextText": "&rarr;"
        }
      }
    ]
  },
  "firstLastLabels": {
    "type": "linked-select",
    "values": [
      {
        "label": "First / Last",
        "values": {
          "firstText": "",
          "lastText": ""
        }
      },
      {
        "label": "« / »",
        "values": {
          "firstText": "&laquo;",
          "lastText": "&raquo;"
        }
      }
    ]
  }
}
