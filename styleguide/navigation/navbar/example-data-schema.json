{
  "navbarClasses": {
    "type": "attribute",
    "key": "class",
    "value": {
      "type": "group",
      "values": [
        {
          "type": "const",
          "value": "navbar"
        },
        {
          "label": "Inverted colors",
          "type": "switch",
          "on": "navbar-inverse",
          "off": "navbar-default"
        },
        {
          "label": "Placement",
          "type": "select",
          "optional": true,
          "values": [
            {
              "label": "Fixed to top",
              "value": "navbar-fixed-top"
            },
            {
              "label": "Fixed to bottom",
              "value": "navbar-fixed-bottom"
            },
            {
              "label": "Static top",
              "value": "navbar-static-top"
            }
          ]
        }
      ]
    }
  },
  "textClasses": {
    "type": "attribute",
    "key": "class",
    "value": {
      "type": "group",
      "values": [
        {
          "type": "const",
          "value": "navbar-text"
        },
        {
          "label": "Right align free text",
          "type": "switch",
          "on": "navbar-right"
        }
      ]
    }
  },
  "menuClasses": {
    "type": "attribute",
    "key": "class",
    "value": {
      "type": "group",
      "values": [
        {
          "type": "const",
          "value": "nav"
        },
        {
          "type": "const",
          "value": "navbar-nav"
        },
        {
          "label": "Right align menu",
          "type": "switch",
          "on": "navbar-right"
        }
      ]
    }
  }
}
